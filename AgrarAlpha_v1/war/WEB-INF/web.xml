<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.gwt.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/agraralpha_v1/greet</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>AgrarAlpha_v1.html</welcome-file>
  </welcome-file-list>
  
    <servlet>
    <servlet-name>ImportHandler</servlet-name>
    <servlet-class>com.gwt.server.ImportHandlerServlet</servlet-class>
  </servlet>
   
  <servlet-mapping>
        <servlet-name>ImportHandlerServlet</servlet-name>
        <url-pattern>/ImportHandlerServlet</url-pattern>
    </servlet-mapping>

</web-app>
